#include<stdio.h>
int main()
{
    int x;
    printf("Enter a year:");
    scanf("%d",&x);
    switch(x%100==0)
    {
    case 1:
        if(x%400==0)
    printf("leap year");
        else
        printf("non leap year");
        break;
    case 0:
        if(x%4==0)
            printf("leap year");
        else
            printf("non leap year");

    }

    return 0;
}
